{
    "name": "angular-xxl",
    "version": "1.0.7",
    "main": "index.js",
    "types": "index.d.ts",
    "repository": "https://github.com/scaljeri/angular-xxl.git",
    "author": "lucascalje <lucas@calje.eu>",
    "license": "MIT",
    "keywords": [
        "angular",
        "router",
        "resolve",
        "parameter",
        "data",
        "decorator"
    ],
    "scripts": {
        "build": "rm -rf bundle && ./node_modules/.bin/tsc && ./node_modules/.bin/tsc -d --lib 'es2015, dom' ./index.ts",
        "prepublish": "yarn build",
        "test": "./node_modules/.bin/nyc mocha",
        "stryker": "./node_modules/.bin/stryker run",
        "coveralls": "./node_modules/.bin/coveralls < coverage/lcov.info",
        "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json 'src/*.ts'"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text-summary",
            "lcov",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    },
    "devDependencies": {
        "@types/chai": "4.1.7",
        "@types/mocha": "5.2.5",
        "@types/sinon-chai": "3.2.2",
        "chai": "4.2.0",
        "coveralls": "3.0.2",
        "mocha": "5.2.0",
        "nyc": "13.1.0",
        "rxjs": "6.3.3",
        "rxjs-tslint": "^0.1.6",
        "rxjs-tslint-rules": "4.13.0",
        "sinon": "7.2.2",
        "sinon-chai": "3.3.0",
        "stryker": "0.33.2",
        "stryker-api": "0.22.1",
        "stryker-babel-transpiler": "0.7.11",
        "stryker-cli": "0.2.0",
        "stryker-html-reporter": "0.16.10",
        "stryker-mocha-framework": "0.13.3",
        "stryker-mocha-runner": "0.15.3",
        "stryker-typescript": "^0.16.1",
        "ts-node": "7.0.1",
        "tslint": "5.12.0",
        "typescript": "3.2.2"
    },
    "peerDependencies": {
        "@angular/core": "^7.0.0"
    }
}
